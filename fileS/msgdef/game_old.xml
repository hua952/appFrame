<?xml version='1.0' encoding='utf-8' ?>
<servers>
	<rpc>
		<test tojs="1" cppDir="../libMsg/src">
			<dataNode>
				<key dataType="udword" commit="主键"> </key>
				<value dataType="udword" commit="数值"> </value>
			</dataNode>

			<strNode RWPack="1">
				<key dataType="udword" commit="主键"> </key>
				<strValue dataType="char" length="64" haveSize="1" zeroEnd="1" worldSize="1" commit="数值"> </value>
			</strNode>
			<doTest>
		    	<ask packetType="pack">
					<Data dataType="dataNode" length="256" haveSize="1"> </Data>
			    </ask>
				<ret packetType="pack">
					<Data dataType="dataNode" length="256" haveSize="1"> </Data>
			    </ret>
		    </doTest>
			<doTestString commit="主键">
		    	<ask packetType="pack" RWPack="1">
					<RoleID dataType="uword" commit="主键"> </RoleID>
					<Data dataType="strNode" length="4" haveSize="0"> </Data>
			    </ask>
				<ret packetType="pack" RWPack="1">
					<RoleID dataType="uword" commit="主键"> </RoleID>
					<Data dataType="strNode" length="4" haveSize="0"> </Data>
			    </ret>
		    </doTestString>

			<exit>
				<ask packetType="pack">
				</ask>
			</exit>
		</test>
	</rpc>
	<app>
		<testServer projectDir="../">
			<procRpc>
				<test>
					<doTest askType="ask"/>
					<doTestString  askType="ask"/>
					<exit askType="ask" exit="1"/>
				</test>
			</proRpc>
		</testServer>
		<testClient projectDir="../">
			<procRpc>
				<test>
					<doTest askType="ret"/>
					<doTestString  askType="ret"/>
				</test>
			</proRpc>
		</testClient>
		<msg2js projectDir="../">
		</msg2js>
	</app>
</servers>

