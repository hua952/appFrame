<?xml version='1.0' encoding='utf-8' ?>
<servers>
	<rpc>
		<test tojs="1" cppDir="../libMsg/src">
			<dataNode>
				<key dataType="udword" commit="主键"> </key>
				<value dataType="udword" commit="数值"> </value>
			</dataNode>

			<strNode RWPack="0">
				<key dataType="udword" commit="主键"> </key>
				<strValue dataType="char" length="62" zeroEnd="0" haveSize="1" worldSize="0" commit="数值"> </strValue>
			</strNode>
			<doTest>
		    	<ask packetType="pack">
					<Data dataType="dataNode" length="256" haveSize="1"> </Data>
			    </ask>
				<ret packetType="pack">
					<Data dataType="dataNode" length="256" haveSize="1"> </Data>
			    </ret>
		    </doTest>
			<doTestString commit="主键">
		    	<ask packetType="pack" RWPack="0">
					<RoleID dataType="uword" commit="主键"> </RoleID>
					<Data dataType="strNode" length="6" haveSize="1"> </Data>
			    </ask>
				<ret packetType="pack" RWPack="0">
					<RoleID dataType="uword" commit="主键"> </RoleID>
					<Data dataType="strNode" length="6" haveSize="1"> </Data>
			    </ret>
		    </doTestString>

			<exit>
				<ask packetType="pack">
				</ask>
			</exit>
		</test>
		<login tojs="1" cppDir="../libMsg/src">
			<addAcc>
					<ask>
						<accName dataType="char" length="32" zeroEnd="1"></accName>
						<passwd  dataType="char" length="32" zeroEnd="1"></passwd>
						<head dataType="uword" ></head>
					</ask>
					<ret>
						<accID dataType="udword" ></accID>
						<result dataType="udword" ></result>
					</ret>
			</addAcc>
			<doLogin>
					<ask>
						<accName dataType="char" length="32" zeroEnd="1"></accName>
						<passwd  dataType="char" length="32" zeroEnd="1"></passwd>
					</ask>
					<ret>
						<accID dataType="udword" ></accID>
						<result dataType="udword" ></result>
						<loginRTtoken dataType="udword"></loginRTtoken>
						<head dataType="uword" ></head>
					</ret>
			</doLogin>
			<kick>
					<ask>
						<reason dataType="udword"></reason>
					</ask>
			</kick>
			<doReLogin>
					<ask>
						<accID dataType="udword"></accID>
						<loginRTtoken dataType="udword"></loginRTtoken>
					</ask>
					<ret>
						<result dataType="udword" ></result>
					</ret>
			</doReLogin>
		</login>
		<sysfun tojs="0">
			<addToChan>
				<ask>
					<chanIndex dataType="udword" ></chanIndex>
					<accID dataType="udword" length="256" haveSize="1" />
				</ask>
			</addToChan>
			<removeFromChan>
				<ask>
					<accID dataType="udword"></accID>
					<chanIndex dataType="udword" ></chanIndex>
				</ask>
			</removeFromChan>
			<delChan>
				<ask>
					<chanIndex dataType="udword"></chanIndex>
				</ask>
			</delChan>
			<accLogin>
				<ask>
					<accID dataType="udword" ></accID>
					<loginToken dataType="udword"></loginToken>
				</ask>
			</accLogin>
			<accOffline>
				<ask>
					<accID dataType="udword" ></accID>
				</ask>
			</accOffline>
			<accReonline>
				<ask>
					<accID dataType="udword" ></accID>
				</ask>
			</accReonline>
		</sysfun>
	</rpc>
</servers>

