<?xml version='1.0' encoding='utf-8' ?>
<servers>
	<projectHome>C:/work/testSysMsg/</projectHome>
	<projectName>testSysMsg</projectName>
	<depIncludeHome>C:/work/include/</depIncludeHome>
	<depLibHome>C:/work/lib/</depLibHome>
	<thirdPartyDir>C:/work/</thirdPartyDir>
	<frameInstallPath>C:/work/appFrameInstall/</frameInstallPath>
	<defMsg file="cChess.xml"/>
	<app>
		<appServer>
			<appArg>detachServerS=1</appArg>
			<module>
				<appServerM>
					<server>
						<appServerTh1 sleepSetp="1" showFps="1" fpsSetp="40000">
							<listen port="11000" teag="1" defRoute="1" name="appServerTh1Len"/>
							<procRpc>
								<move askType="ask"/>
							</procRpc>
						</appServerTh1>
						<appServerTh2 sleepSetp="1" showFps="1" fpsSetp="40000">
							<procRpc>
								<roomChannel askType="ask"/>
								<roomChatTxt askType="ask"/>
							</procRpc>
						</appServerTh2>
						<appServerTh3 autoRun="0" showFps="0">
						</appServerTh3>
					</server>
				</appServerM>
			</module>
		</appServer>
		<appClient>
			<appArg>detachServerS=1</appArg>
			<module>
				<appClientM2>
					<server>
						<appClientM2Th sleepSetp="1" showFps="1" fpsSetp="40000">
							<procRpc>
								<regret askType="ask"/>
							</procRpc>
						</appClientM2Th>
					</server>
				</appClientM2>
				<appClientM1>
					<server>
						<appClientM1Th1 sleepSetp="1" showFps="1" fpsSetp="40000">
							<procRpc>
								<manualList askType="ret"/>
								<regret askType="ret"/>
								<move askType="ret"/>
								<roomChannel askType="ret"/>
								<roomChatTxt askType="ask"/>
							</procRpc>
						</appClientM1Th1>
						<appClientM1Th2 sleepSetp="1" showFps="1" fpsSetp="40000">
							<connect ip="127.0.0.1" port="11000" target="appServerTh1" defRoute="1"  name="appClientM1Th2Con" targetEndPoint="appServerTh1Len" teag="1"/>
							<procRpc>
								<manualList askType="ask"/>
							</procRpc>
						</appClientM1Th2>
						<appClientM1Th3 autoRun="0">
						</appClientM1Th3>
					</server>
				</appClientM1>
			</module>
		</appClient>
	</app>
</servers>

